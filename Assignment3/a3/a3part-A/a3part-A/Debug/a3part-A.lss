0001dd f7f1                      		brne dlyms
                                 
0001de 91cf                      	pop YL
0001df 91df                      	pop YH
0001e0 913f                      	pop DREG
0001e1 911f                      	pop TEMP2
0001e2 910f                      	pop TEMP
0001e3 9508                      	ret
